.el-button--more,.el-button--more:active,.el-button--more:focus,.el-button--more:hover{
  border: none;
  background-color: transparent;
  color: #5F6A7A;
  padding: 4px;
  .el-icon-alimore{
    font-size: 22px;
    color: #5F6A7A;
  }
}

//el-tree
.el-tree--highlight-current .el-tree-node.is-current>.el-tree-node__content{
  color: #5497FC;
}

//el-form
.el-form{
  .el-select{
    width: 100%;
  }
}

.el-dropdown-menu{
  .el-dropdown-menu__item{
    line-height: 40px;
  }
}

.el-menu--popup-bottom-start{
  margin-top: 12px;
}

.el-menu--popup-bottom-start{
  &:before{/*.before为ie6下用到*/
    content: ''; /*伪类下的属性*/
    display:block;
    position:absolute;
    top:-18px;
    left:35px;
    border:10px;
    border-style:dashed dashed solid dashed;
    border-color:transparent transparent #fff transparent;
  }
}

.cardFlex {
  max-height: calc(100% - 160px);
  overflow-y: auto;
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(300px,1fr));
  justify-items: center;
  grid-gap: 20px 25px;
  padding-right: 5px;
  padding-left:1px;

  .card {
    cursor: pointer;
    position: relative;
    width: 100%;
    height: 180px;

    ::v-deep .el-card__body {
      padding: 15px 20px;
    }

    .card-more {
      position: absolute;
      right: 0;
      top: 12px;
    }

    .title {
      color: #37485D;
      font-size: 16px;
      font-weight: bold;
      margin-bottom: 15px;
    }
    .wrap {
      .tip{
        color: #8A8A8A;
      }
      color: #37485D;
      font-size: 14px;
    }
    .wrap:not(:last-child){
      margin-bottom: 10px;
    }
  }
}

.el-tag{
  height: 30px;
}

::-webkit-scrollbar{width:14px;}
::-webkit-scrollbar-track{background-color:#E4E9F1;border-radius: 0px 0px 7px 7px;}
::-webkit-scrollbar-thumb{background-color:#AAB4C2;border-radius: 7px;}

.view-dialog{
  .el-form-item{
    margin-bottom: 5px;
    .el-form-item__label{
      font-size: 16px;
      color: #8A8A8A;
      padding-right: 22px;
    }
    .el-form-item__content{
      font-size: 16px;
      color: #37485D;
    }
  }
}

.view-image{
  width: 146px;
  height: 146px;
  margin-right: 10px;
  margin-bottom: 10px;
}

.el-image-viewer__close{
  color: #fff;
}