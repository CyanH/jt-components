.head-container {
  padding-bottom: 10px;
  .filter-item {
    display: inline-block;
    vertical-align: middle;

    input {
      height: 40px;
      line-height: 40px;
    }
  }
  .el-form-item-label {
    margin: 0 3px 9px 0;
    display: inline-block;
    text-align: right;
    vertical-align: middle;
    font-size: 14px;
    color: #606266;
    line-height: 30.5px;
    padding: 0 7px 0 7px;
  }


  .date-item {
    vertical-align: middle;
    margin-right: 8px;
    width: 270px;
  }
}
.el-avatar {
  display: inline-block;
  text-align: center;
  background: #ccc;
  color: #fff;
  white-space: nowrap;
  position: relative;
  overflow: hidden;
  vertical-align: middle;
  width: 32px;
  height: 32px;
  line-height: 32px;
  border-radius: 16px;
}

.my-code{
  position: relative;
  padding: 15px;
  line-height: 20px;
  border-left: 5px solid #ddd;
  color: #333;
  font-size: 14px
}

.el-tabs{
  margin-bottom: 25px;
}


.el-table__row{
  .el-button{
    margin-right:5px
  }
  .el-button+.el-button {
    margin-left: 0 !important;
  }
}

.el-table{
  th{
    background-color: #E4EBF3;
    color: #37485D;
  }
  .el-table__fixed-right-patch{
    background-color: #E4EBF3;
  }
}

//el-dialog
.el-dialog{
  border-radius: 6px;
  top: 50%;
  transform: translateY(-50%);
  margin-top: 0 !important;
  .el-dialog__header{
    padding: 30px 30px 0;
    .el-dialog__title{
      font-size: 24px;
      font-weight: bold;
      color: #37485D;

      .dialog-fullscreen{
        position: absolute;
        top: 20px;
        right: 65px;
        padding: 0;
        color: #909399;
      
        i{
          font-size: 16px;
        }
      }
    }
  }
  .el-dialog__body{
    padding:30px 30px 70px;
  }
  .el-dialog__footer{
    position: absolute;
    bottom: 0;
    width: 100%;
  }
  .dialog-footer{
    text-align: right;
  }
}

