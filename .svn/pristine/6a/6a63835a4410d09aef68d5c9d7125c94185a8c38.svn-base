.wraper {
    width: 100%;
    height: 100%;
    padding: 20px;
    background: rgba(247, 248, 247, 1);

    .content {
        width: calc(100% - 40px);
        height: calc(100% - 40px);
        background: #fff;
        border-radius: 5px;

        .content-body {
            padding: 40px 20px;

            .top-search {
                width: 100%;
                display: flex;

                .search-input {
                    width: 200px;
                }

                .leftMargin {
                    margin-left: 20px;
                }

                .serach-btn {
                    width: 95px;
                    height: 42px;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    font-size: 16px;
                    color: rgba(37, 193, 57, 1);
                    border-color: rgba(37, 193, 57, 1);
                }

                .add-btn {
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    font-size: 16px;
                    background: rgba(37, 193, 57, 1);
                    border-color: rgba(37, 193, 57, 1);
                }

                .btn-fontSize {
                    font-size: 16px;
                }
            }

            .select-search {
                display: flex;
                margin-top: 30px;

                .select-label {
                    width: 90px;
                    height: 40px;
                    text-align: center;
                    line-height: 40px;
                    color: rgba(96, 98, 102, 1);
                    font-size: 18px;
                    font-weight: 500;
                    border: 1px solid #eee;
                    cursor: pointer;
                }

                .select-label-active {
                    width: 90px;
                    height: 40px;
                    text-align: center;
                    line-height: 40px;
                    color: rgba(96, 98, 102, 1);
                    font-size: 18px;
                    font-weight: 600;
                    border: 1px solid #eee;
                    cursor: pointer;
                    background: rgba(37, 193, 57, 1);
                    color: #fff;
                    border-right: none;
                }

                .select-label:nth-child(2) {
                    border-left: none;
                    border-right: none;
                }

                .select-label:nth-child(3) {
                    // border-left: none;
                    border-right: none;
                }

                .select-label:nth-child(4) {
                    // border-left: none;
                    border-right: none;
                }
            }

            .result-table {
                margin-top: 20px;
            }
        }
    }
}